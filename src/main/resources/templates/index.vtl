<h2 class="well">Your Dictionary</h2>

<form action="/dictionary/new" method="get" id="add-btn">
  <button type="submit" class="btn btn-info">Add New Word</button>
</form>

<hr>

<table class="table table-striped">
  #foreach ($word in $words)
    <tr>
      <td>$word.getWord()</td>
      <td>
        <form action="/word/$word.getId()">
          <button type="submit" class="btn btn-success">View Definitions</button>
        </form>
      </td>
    </tr>
  #end
</table>
